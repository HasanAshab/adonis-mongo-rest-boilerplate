{
  "typescript": true,
  "commands": [
    "./commands",
    "@adonisjs/core/build/commands/index.js",
    "@adonisjs/repl/build/commands",
    "@adonisjs/bouncer/build/commands",
    "@adonisjs/lucid/build/commands"
  ],
  "directories": {
    "providers": "app/Providers"
  },
  "namespaces": {
    "httpControllers": "App/Http/Controllers",
    "validators": "App/Http/Validators",
    "middleware": "App/Http/Middleware"
  },
  "exceptionHandlerNamespace": "App/Exceptions/Handler",
  "aliases": {
    "App": "app",
    "Config": "config",
    "Database": "database",
    "Contracts": "contracts",
    "Tests": "tests"
  },
  "preloads": [
    "./start/kernel",
    "./start/bouncer",
    {
      "file": "./start/events",
      "environment": [
        "web",
        "test"
      ]
    },
    "./start/limiter"
  ],
  "providers": [
    "./app/Providers/AppProvider",
    "@adonisjs/core",
    "@adonisjs/redis",
    "./app/Providers/CacheProvider",
    "./app/Providers/HashProvider",
    "@adonisjs/bouncer",
    "@adonisjs/ally",
    "./app/Providers/TwilioProvider",
    "./app/Providers/ValidationProvider",
    "adonis-recaptcha2",
    "@adonisjs/limiter",
    "./app/Providers/RouteServiceProvider",
    "@adonisjs/lucid",
    "@adonisjs/auth",
    "@adonisjs/attachment-lite"
  ],
  "aceProviders": [
    "@adonisjs/repl"
  ],
  "tests": {
    "suites": [
      {
        "name": "functional",
        "files": [
          "tests/functional/**/*.spec(.ts|.js)"
        ],
        "timeout": 60000
      }
    ]
  },
  "testProviders": [
    "@japa/preset-adonis/TestsProvider"
  ]
}